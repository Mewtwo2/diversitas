
<body id="charges-new-body" style="text-align:center;"> <!-- temporary styling for readability -->

<div class="toplevel-container">
  <a href="/"><img style="height:8vh;" src="data:image/svg+xml;base64," /></a>
</div><br>

<h1>Log In</h1>
<%= form_for(:session, url: login_path) do |f| %>
  <label>Email:</label><br><br>
  <%= f.email_field :email, placeholder: 'Email', required: true %><br><br>
  <label>Password:</label><br><br>
  <%= f.password_field :password, placeholder: 'Password', required: true %><br><br>
  <%= f.submit 'Log In' %>
<% end %>
</body>
